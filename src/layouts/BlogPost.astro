---
import Layout from './Layout.astro'

const { frontmatter } = Astro.props
const bannerEnd = frontmatter.bannerEnd

let Banner = null
if (bannerEnd) {
  Banner = (await import(`../banners/${bannerEnd}.astro`)).default
}
---

<Layout title={frontmatter.title}>
  <slot />

  {bannerEnd && Banner && <Banner />}
</Layout>
